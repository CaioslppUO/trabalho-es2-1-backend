{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "minLength": 3
    },
    "email": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "minLength": 3
    },
    "cpf": {
      "type": "string",
      "maxLength": 45,
      "minLength": 10,
      "required": true,
      "unique": true
    },
    "service_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-order.service-order",
      "mappedBy": "client"
    }
  }
}

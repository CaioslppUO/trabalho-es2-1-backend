{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "client",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "minLength": 3
    },
    "cpf": {
      "type": "string",
      "required": true,
      "maxLength": 45,
      "minLength": 10,
      "unique": true,
      "regex": "[0-9]{3}[0-9]{3}[0-9]{3}[0-9]{2}"
    },
    "email": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "minLength": 1,
      "regex": "[a-zA-Z0-9._-]{3,}@[a-zA-Z0-9.-]{3,}\\.[a-zA-Z]{2,4}"
    },
    "phones": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::phone.phone",
      "mappedBy": "client"
    }
  }
}
